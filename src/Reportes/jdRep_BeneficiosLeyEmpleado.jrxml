	<queryString>
		<![CDATA[select distinct concat(e.ape_paterno, ' ',e.ape_materno,' ',e.nombres) as "Empleado",b.nombre,sum(db.monto) from detalle_beneficio db
inner join beneficio b on b.id=db.idbeneficio
inner join boleta_pago bp on bp.id=db.idpago
inner join contrato c on c.id=bp.idcontrato
inner join empleado e on e.id=c.idempleado
group by concat(e.ape_paterno, ' ',e.ape_materno,' ',e.nombres),b.nombre
order by 1;]]>
	</queryString>
