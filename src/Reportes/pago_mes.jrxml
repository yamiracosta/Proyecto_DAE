	<queryString>
		<![CDATA[SELECT em.id, em.nombres || ' ' || em.ape_paterno || ' ' || em.ape_materno AS nombre, 
con.salarioacordado, bol.fechapago, bol.sueldopagado FROM empleado em 
JOIN contrato con ON con.idempleado = em.id 
JOIN horario_trabajo hor ON hor.id = con.idhorariotrabajo 
JOIN boleta_pago bol ON bol.idcontrato = con.id WHERE em.estado = 'V' AND em.id = $P{is_empleado} 
AND bol.fechapago IS NOT NULL]]>
	</queryString>
