	<parameter name="idempleado" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[--DATOS DEL EMPLEADO
SELECT e.numdocumento,td.nombre as tipo_documento,concat(e.ape_paterno,' ',e.ape_materno,' ',e.nombres) as nombres_empleado,
coalesce(correo,'No cuenta') as correo,e.telefono,te.nombre as tipo_empleado,
case
	when e.idempleadoempleado is null then 'No cuenta'
	else (select concat(ape_paterno,' ',ape_materno,' ',nombres) from empleado where id=e.idempleadoempleado)
end as nombre_superior,
case
	when e.estado='V' then 'Vigente'
	when e.estado='N' then 'No vigente'
end as estadoempleado,
case
	when e.tienehijos=true then 'Sí'
	when e.tienehijos=false then 'No'
end as tienehijos
from empleado e
inner join tipo_empleado te on te.id=e.idtipoempleado
inner join tipo_documento td on td.id=e.idtipodocumento
inner join area a on a.id=te.idarea
where e.id=$P{idempleado};]]>
	</queryString>
