	<queryString>
		<![CDATA[SELECT
    e.id AS id_empleado,
    e.nombres || ' ' || e.ape_paterno AS empleado,
    TO_CHAR(bp.fechapago, 'DD/MM/YYYY') AS fechapago,
    TO_CHAR(bp.fechapago, 'Month') AS mes,
    EXTRACT(YEAR FROM bp.fechapago) AS anio,
    bp.montototal AS montototal_pago,
    bp.sueldopagado AS sueldopagado,
    bp.montodiastrabajados AS dias_trabajados
FROM
    public.empleado e
JOIN
    public.boleta_pago bp ON e.id = bp.idoperador
WHERE
    bp.fechapago IS NOT NULL
    AND bp.montototal IS NOT NULL
    AND bp.sueldopagado IS NOT NULL
    AND bp.montodiastrabajados IS NOT NULL
ORDER BY
    e.nombres, e.ape_paterno;]]>
	</queryString>
