	<queryString>
		<![CDATA[select c.fechainicio,c.fechatermino,tc.nombre,c.salarioacordado,coalesce(c.responsabilidades,'Ninguna') as responsabilidades,
case
	when c.estado = 'V' then 'Vigente'
	when c.estado = 'T' then 'Culminado'
	when c.estado = 'A' then 'Anulado'
end as estadocontrato,
pp.nombre,c.fecharegistro,
case
	when c.vacacionespagadas=true then 'Sí'
	when c.vacacionespagadas=false then 'No'
end as vacacionespagadas,
concat(ht.turno,' (',ht.horaentrada,' - ',ht.horasalida,')') as horariotrabajo
from contrato c
inner join tipo_contrato tc on tc.id=c.idtipocontrato
inner join periodo_pago pp on pp.id=c.idperiodopago
inner join horario_trabajo ht on ht.id=c.idhorariotrabajo
inner join empleado e on e.id=c.idempleado
inner join tipo_empleado te on te.id=e.idtipoempleado
where e.id=$P{idempleado_reporteprincipal} and c.estado=$P{vigencia_cont};]]>
	</queryString>
